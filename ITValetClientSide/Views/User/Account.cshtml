        function createAvailabilityTable() {
            const today = new Date().getDay();
            const daysOfWeek = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"]; // Include "Sunday" at the end
            const timeSlots = ["Morning", "Afternoon", "Evening", "Night"];

            const tbody = document.querySelector("#addWeeklyAvailabilitytbody"); // Get the tbody element

            // Loop through the days of the week
            for (let i = 0; i < daysOfWeek.length; i++) {
                const day = daysOfWeek[i];
                const row = document.createElement("tr"); // Create a new table row

                const dayHeader = document.createElement("td"); // Create a cell for the day name
                const dayHeading = document.createElement("h6");
                dayHeading.textContent = day;
                dayHeader.appendChild(dayHeading);
                row.appendChild(dayHeader);

                // Loop through the time slots
                for (const slot of timeSlots) {
                    const cell = document.createElement("td"); // Create a cell for the checkbox

                    const checkboxDiv = document.createElement("div");

                    checkboxDiv.className = "custom-control custom-switch";

                    const checkbox = document.createElement("input"); // Create the checkbox
                    checkbox.type = "checkbox";
                    checkbox.className = "custom-control-input";
                    checkbox.id = `customSwitchesFor${day}${slot}`;

                    const label = document.createElement("label"); // Create the label
                    label.className = "custom-control-label";
                    label.setAttribute("for", `customSwitchesFor${day}${slot}`);

                    checkboxDiv.appendChild(checkbox);
                    checkboxDiv.appendChild(label);
                    cell.appendChild(checkboxDiv);
                    //if (i < today) {
                    //    // If the day is before today, disable the checkbox
                    //    checkbox.disabled = true;
                    //}

                    row.appendChild(cell); // Add the cell to the row
                }

                tbody.appendChild(row); // Add the row to the tbody
            }
        }


    </script>

    <script>
        function updateAvailability(slotEncId, slotDecId, slotNo) {
            $("#checkAvailabilityLoader").css("display", "block");
            $("#checkWeeklyAvailability").css("display", "none");
            const isChecked = $("#" + slotDecId).prop("checked");
            var Slot1 = "";
            var Slot2 = "";
            var Slot3 = "";
            var Slot4 = "";
            if (!isChecked) {
                if (slotNo == "1") {
                    Slot1 = "0";
                }
                if (slotNo == "2") {
                    Slot2 = "0";
                }
                if (slotNo == "3") {
                    Slot3 = "0";
                }
                if (slotNo == "4") {
                    Slot4 = "0";
                }
            }
            else {
                if (slotNo == "1") {
                    Slot1 = "1";
                }
                if (slotNo == "2") {
                    Slot2 = "2";
                }
                if (slotNo == "3") {
                    Slot3 = "3";
                }
                if (slotNo == "4") {
                    Slot4 = "4";
                }
            }
            const requestData = {
                UserAvailableSlotEncId: slotEncId,
                Slot1: Slot1,
                Slot2: Slot2,
                Slot3: Slot3,
                Slot4: Slot4
            };

            $.ajax({
                type: 'PUT',
                headers: {
                    'Authorization': Token
                },
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(requestData),
                url: projectBaseUrl + "User/PostUpdateUserAvailableSlot",
                success: function (response) {
                    triggerSweetAlert(response.message, "@ProjectVariables.SuccessColor");
                    checkAvailability();
                },
                error: function (response) {
                    triggerSweetAlert(response.message, "@ProjectVariables.DangerColor");
                }
            });
        }

        function addAvailability() {
            // Disable the button
            $("#addAvailabilityBtn").prop("disabled", true);
            const daysOfWeek = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"]; // Include "Sunday" at the end
            const timeSlots = ["Morning", "Afternoon", "Evening", "Night"];
            var availabilityData = [];

            for (const day of daysOfWeek) {
                var s1 = "";
                var s2 = "";
                var s3 = "";
                var s4 = "";
                for (var i = 0; i < timeSlots.length; i++) {
                    const checkboxId = `customSwitchesFor${day}${timeSlots[i]}`;
                    const isChecked = $("#" + checkboxId).prop("checked");
                    if (i == 0)
                        s1 = isChecked ? "1" : "0";
                    if (i == 1)
                        s2 = isChecked ? "2" : "0";
                    if (i == 2)
                        s3 = isChecked ? "3" : "0";
                    if (i == 3)
                        s4 = isChecked ? "4" : "0";
                }
                var x = {
                    DateTimeOfDay: day,
                    Slot1: s1,
                    Slot2: s2,
                    Slot3: s3,
                    Slot4: s4,
                    UserId: getUserIdFromViewBag
                };
                availabilityData.push(x);
            }

            $.ajax({
                type: 'POST',
                headers: {
                    'Authorization': Token
                },
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(availabilityData),
                url: projectBaseUrl + "User/PostAddUserAvailableSlots",
                success: function (response) {
                    triggerSweetAlert("The time slot for availability has been modified", "@ProjectVariables.SuccessColor");
                    checkAvailability();
                },
                error: function (response) {
                    triggerSweetAlert(response.message, "@ProjectVariables.DangerColor");
                }
            });
            console.log(availabilityData);
        }

