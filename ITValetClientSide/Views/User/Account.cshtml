                cache: false,
                success: function (response) {
                    var appendOptions = '';
                    $.each(response, function (key, value) {
                        appendOptions += '<option value=' + key + '>' + value + '</option>';
                    })
                    $("#updRegionTimeZones").append(appendOptions);
                },
                error: function () {
                },
                complete: function () {
                    $("#updRegionTimeZones").val('@ViewBag.UserRecord.Timezone').change();
                }
            });
        }
    </script>
}